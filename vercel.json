{
    "version": 2,
    "buildCommand": "rm -rf .next && vc build && vc deploy --prebuilt",
    "routes": [
        {
          "src": "/(.*)",
          "dest": "api/index.py",
          "headers": {
            "Access-Control-Allow-Origin": "*",
            "Access-Control-Allow-Methods": "GET,POST,PUT,DELETE,OPTIONS",
            "Access-Control-Allow-Headers": "Content-Type, Authorization"
          }
        }
      ],
      "functions": {
        "api/*": {
          "maxDuration": 100
        }
      },
      "env": {
        "APP_MODULE": "app:api"
      }

  }